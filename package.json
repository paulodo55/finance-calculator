{
  "name": "financeai-calculator",
  "version": "1.0.0",
  "description": "AI-powered financial calculator for college students - educational tool for learning financial formulas",
  "main": "index.html",
  "scripts": {
    "dev": "npx serve .",
    "build": "mkdir -p public && cp index.html styles.css script.js public/",
    "start": "npx serve . -p 3000",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "finance",
    "calculator",
    "education",
    "ai",
    "student",
    "formulas",
    "simple-interest",
    "compound-interest",
    "annuities",
    "loans",
    "mortgages"
  ],
  "author": "FinanceAI Team",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.2.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/financeai-calculator.git"
  },
  "homepage": "https://financeai-calculator.vercel.app",
  "vercel": {
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ]
  }
} 